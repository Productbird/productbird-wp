{
    "name": "productbird/productbird",
    "description": "Productbird WordPress plugin providing AI tools to help ecommerce owners get more done.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Productbird",
            "email": "team@productbird.ai"
        }
    ],
    "require": {
        "php": ">=8.2",
        "composer/installers": "^2.2",
        "kucrut/vite-for-wp": "^0.11.5"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "phpcompatibility/php-compatibility": "^9.3",
        "woocommerce/woocommerce-sniffs": "^1.0",
        "wp-coding-standards/wpcs": "^3.0",
        "squizlabs/php_codesniffer": "^3.7",
        "php-stubs/woocommerce-stubs": "*",
        "wp-cli/i18n-command": "^2.6",
        "php-stubs/acf-pro-stubs": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Productbird\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Productbird\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "lint": "phpcs",
        "lint:fix": "phpcbf"
    }
}